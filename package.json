{
  "name": "butter-component-list",
  "version": "0.6.0",
  "description": "A base React Component for Butter Project",
  "main": "src/index.jsx",
  "scripts": {
    "prepublish": "npm run build",
    "install": "PATH=$PATH:../.bin webpack --config ../butter-component-builder/webpack/webpack.prod.config.js --progress --profile --colors",
    "build": "PATH=$PATH:../.bin webpack --config node_modules/butter-component-builder/webpack/webpack.prod.config.js --progress --profile --colors",
    "start": "node node_modules/butter-component-builder/server/server.js",
    "lint": "eslint src"
  },
  "build": {
    "command": "webpack --config webpack.config.js --progress --profile --colors",
    "env": {
      "NODE_ENV": "production"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buttercomponents/butter-component-list.git"
  },
  "keywords": [
    "react",
    "components",
    "butterproject",
    "butter"
  ],
  "author": "Niv Sardi <x@btn.sh>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/buttercomponents/butter-component-list/issues"
  },
  "homepage": "https://github.com/buttercomponents/butter-component-list#readme",
  "devDependencies": {
    "butter-component-builder": "buttercomponents/butter-component-builder"
  }
}
